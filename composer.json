{
    "name": "nswdpc/silverstripe-chimple",
    "description": "Chimple - Simple Mailchimp handling in Silverstripe",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "nsw",
        "mailchimp",
        "simple",
        "chimp"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "James",
            "role": "Developer"
        },
        {
            "name": "Mark",
            "role": "Developer"
        },
        {
            "name": "Symbiote",
            "role": "Contributors"
        }
    ],
    "extra": {
        "expose": [
            "client/static"
        ]
    },
    "require": {
        "dnadesign/silverstripe-elemental": "^6",
        "silverstripe/asset-admin": "^3",
        "silverstripe/framework": "^6",
        "silverstripe/siteconfig": "^6",
        "symbiote/silverstripe-queuedjobs": "^6",
        "drewm/mailchimp-api": "^2.5",
        "symbiote/silverstripe-multivaluefield": "dev-7 || ^7"
    },
    "require-dev": {
        "cambis/silverstripe-rector": "^2",
        "cambis/silverstan": "^2",
        "friendsofphp/php-cs-fixer": "^3",
        "phpunit/phpunit": "^11.5",
        "phpstan/phpstan": "^2",
        "rector/rector": "^2",
        "nswdpc/ci-files": "dev-v-4",
        "vlucas/phpdotenv": "^5"
    },
    "autoload": {
        "psr-4": {
            "NSWDPC\\Chimple\\Models\\": "src/Models/",
            "NSWDPC\\Chimple\\Models\\Elements\\" : "src/Models/Elements/",
            "NSWDPC\\Chimple\\Controllers\\" : "src/Controllers/",
            "NSWDPC\\Chimple\\Extensions\\" : "src/Extensions/",
            "NSWDPC\\Chimple\\Jobs\\" : "src/Jobs/",
            "NSWDPC\\Chimple\\Services\\" : "src/Services/",
            "NSWDPC\\Chimple\\Forms\\" : "src/Forms/",
            "NSWDPC\\Chimple\\Exceptions\\" : "src/Exceptions/",
            "NSWDPC\\Chimple\\Traits\\": "src/Traits/",
            "NSWDPC\\Chimple\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true,
            "silverstripe/recipe-plugin": true,
            "phpstan/extension-installer": true
        }
    }
}
